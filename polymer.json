{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-data.html",
    "src/my-accounts.html",
    "src/add-account.html",
    "src/mod-account.html",
    "src/my-pockets.html",
    "src/add-pocket.html",
    "src/mod-pocket.html",
    "src/my-movements.html",
    "src/my-login.html",
    "src/sign-in.html",
    "src/hi-welcome.html",
    "src/my-view404.html",
    "login.html",
    "register.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "bower_components/polymer/*",
    "manifest.json"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    }
  ]
}